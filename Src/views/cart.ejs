<!-- @format -->

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<link rel="stylesheet" href="/static/cart.css" />
		<link
			href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css"
			rel="stylesheet"
			integrity="sha384-F3w7mX95PdgyTmZZMECAngseQB83DfGTowi0iMjiWaeVhAn4FJkqJByhZMI3AhiU"
			crossorigin="anonymous"
		/>
		<link
			rel="stylesheet"
			href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
		/>
		<title>OverStocks.com</title>
	</head>
	<body>
		<div class="container-fluid">
			<div class="row">
				<div class="mt-2 ms-4">
					<div id="ship" style="font-size: 1vw">Ships to:</div>
					<div style="font-size: 1vw">
						<img
							src="data:image/png;base64,"
							class="img-fluid"
							id="flag"
							alt="Responsive image"
						/>

						<div class="fw-bold">INDIA</div>
					</div>
				</div>
			</div>
			<hr />
		</div>

		<div class="container-fluid">
			<div class="row">
				<div
					class="
						col-xl-2
						col-lg-3
						col-md-4
						col-sm-6
						col-6
						col-lg-2
						col-md-2
						col-sm-2
						col-2
					"
					style="font-size: 3vw"
				>
					<img
						src="https://ak1.ostkcdn.com/img/mxc/OSTK_MAIN_LOGO_20181127KCH.jpg"
						id="Mainlogo"
					/>
				</div>
				<div class="col-xl-10 col-lg-10 col-md-10 col-sm-10 col-10">
					<nav class="navbar navbar-expand-lg navbar-light bg-light">
						<div class="container-fluid">
							<div class="col-xl-9 col-lg-9 col-md-10 col-sm-10">
								<form class="d-flex">
									<input
										class="form-control me-2"
										type="search"
										placeholder="Search"
										aria-label="Search"
									/>
									<button class="btn btn-outline-success" type="submit">
										Search
									</button>
								</form>
							</div>
							<div class="col-xl-3">
								<div class="row">
									<div class="col-xl-1 ms-xl-5">
										<button
											class="navbar-toggler"
											type="button"
											data-bs-toggle="collapse"
											data-bs-target="#navbarSupportedContent"
											aria-controls="navbarSupportedContent"
											aria-expanded="false"
											aria-label="Toggle navigation"
										>
											<span class="navbar-toggler-icon"></span>
										</button>
										<div
											class="collapse navbar-collapse"
											id="navbarSupportedContent"
										>
											<a class="navbar-brand" href="#"
												><img
													src="https://static.thenounproject.com/png/3542376-200.png"
													width="30px"
													class="ms-xl-2"
												/>
												<div style="font-size: 14px">Account</div></a
											>
											<ul class="navbar-nav me-auto mb-2 mb-lg-0">
												<li class="nav-item ms-xl-2">
													<a
														class="nav-link active"
														aria-current="page"
														href="#"
														><img
															src="data:image/png;base64,"
															width="30px"
														/>
														<div style="font-size: 14px">Lists</div></a
													>
												</li>
												<li class="nav-item ms-xl-3">
													<a class="nav-link active" href="#"
														><img
															src="data:image/png;base64,"
															width="30px"
														/>
														<div style="font-size: 14px">Cart</div></a
													>
												</li>
											</ul>
										</div>
									</div>
								</div>
							</div>
						</div>
					</nav>
				</div>
			</div>
		</div>
		<hr />

		<hr />
		<div class="container">
			<div class="row"><div style="font-size: 30px">Shopping Cart</div></div>
			<div class="row">
				<div class="col-9 box mt-4" id="here"></div>
				<div class="col-3 box mt-4">
					<a
						href=""
						id="total"
						style="
							font-size: 25px;
							text-decoration: none;
							color: black;
							margin-left: 30px;
						"
						>Your Total:
						<a
							href=""
							id="cartto"
							style="font-size: 25px; text-decoration: none; color: red"
						></a
					></a>
					<div>
						<button
							type="button"
							class="btn btn-warning"
							style="margin-left: 50px"
						>
							<a
								href="/home/payment"
								style="text-decoration: none; font-size: 20px; color: black"
								>Checkout</a
							>
						</button>
					</div>
				</div>
			</div>
		</div>
	</body>
</html>
<script
	src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.bundle.min.js"
	integrity="sha384-/bQdsTh/da6pkI1MST/rWKFNjaCP5gBSY4sEBT38Q/9RBh9AH40zEOg7Hlq2THRZ"
	crossorigin="anonymous"
></script>
<script>
	let cart = JSON.parse(localStorage.getItem('cartItems'));
	let here = document.getElementById('here');
	console.log(cart);
	let res = 0;
	cart.forEach(el => {
		res += parseInt(el.price);

		let div = document.createElement('div');
		div.innerHTML = `<div class="row" id="cart">
						<div class="col-4">
							<div>
								<img
									src="${el.image}"
									alt=""
									width="270px"
									height="250px"
								/>
							</div>
						</div>
						<div class="col-8">
							<div  class="name">
								${el.name}
							</div>
							<div  class="name">Charcoal Black</div>
							<del  class="name">INR 36,874.64</del>
							<div  class="name">Sale INR ${el.price}</div>
							<div  class="name">25% Savings</div>
							<div  class="name">115 people have this in their cart</div>
							<div  class="name">
								<select name="" id="">
									<option value="1">1</option>
									<option value="2">2</option>
								</select>
							</div>
							<a href=""  class="name">REMOVE</a>
							<a href=""  style="margin-left:20px">SAVE FOR LATER</a>
						</div>
					</div>`;
		here.append(div);
	});
	document.getElementById('cartto').innerHTML = res;
</script>
